<?xml version="1.0" encoding="UTF-8"?>
<h:html xmlns="http://www.w3.org/2002/xforms" xmlns:ev="http://www.w3.org/2001/xml-events" xmlns:h="http://www.w3.org/1999/xhtml" xmlns:jr="http://openrosa.org/javarosa" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <h:head>
    <h:title>Sample Form 1</h:title>
    <model>
      <instance>
        <data id="*form1*" version="*formver1*">
          <*qcode1*/>
          <*qcode2*/>
          <*qcode3*/>
          <*qcode4*/>
          <*qcode5*/>
          <*qcode6*/>
          <*qcode7*_1/>
          <*qcode7*_2/>
          <*qcode8*/>
          <*qcode9*/>
          <*qcode10*/>
          <*qcode11*/>
        </data>
      </instance>
      <itext>
        <translation lang="English">
          <text id="*qcode1*:label">
            <value>Text Question Title 1</value>
          </text>
          <text id="*qcode1*:hint">
            <value>Question Hint 1</value>
          </text>
          <text id="*qcode2*:label">
            <value>Long Text Question Title 2</value>
          </text>
          <text id="*qcode2*:hint">
            <value>Question Hint 2</value>
          </text>
          <text id="*qcode3*:label">
            <value>Integer Question Title 3</value>
          </text>
          <text id="*qcode3*:hint">
            <value>Question Hint 3</value>
          </text>
          <text id="*qcode4*:label">
            <value>Decimal Question Title 4</value>
          </text>
          <text id="*qcode4*:hint">
            <value>Question Hint 4</value>
          </text>
          <text id="*qcode5*:label">
            <value>Location Question Title 5</value>
          </text>
          <text id="*qcode5*:hint">
            <value>Question Hint 5</value>
          </text>
          <text id="*qcode6*:label">
            <value>Select One Question Title 6</value>
          </text>
          <text id="*qcode6*:hint">
            <value>Question Hint 6</value>
          </text>
          <text id="*qcode7*_1:label">
            <value>Multilevel Select One Question Title 7 - Kingdom</value>
          </text>
          <text id="*qcode7*_1:hint">
            <value>Question Hint 7</value>
          </text>
          <text id="*qcode7*_2:label">
            <value>Multilevel Select One Question Title 7 - Species</value>
          </text>
          <text id="*qcode7*_2:hint">
            <value>Question Hint 7</value>
          </text>
          <text id="*qcode8*:label">
            <value>Select Multiple Question Title 8</value>
          </text>
          <text id="*qcode8*:hint">
            <value>Question Hint 8</value>
          </text>
          <text id="*qcode9*:label">
            <value>Datetime Question Title 9</value>
          </text>
          <text id="*qcode9*:hint">
            <value>Question Hint 9</value>
          </text>
          <text id="*qcode10*:label">
            <value>Date Question Title 10</value>
          </text>
          <text id="*qcode10*:hint">
            <value>Question Hint 10</value>
          </text>
          <text id="*qcode11*:label">
            <value>Time Question Title 11</value>
          </text>
          <text id="*qcode11*:hint">
            <value>Question Hint 11</value>
          </text>
          <text id="*optcode1*">
            <value>Cat</value>
          </text>
          <text id="*optcode2*">
            <value>Dog</value>
          </text>
          <text id="*optcode6*">
            <value>Animal</value>
          </text>
          <text id="*optcode3*">
            <value>Plant</value>
          </text>
          <text id="*optcode9*">
            <value>Cat</value>
          </text>
          <text id="*optcode10*">
            <value>Dog</value>
          </text>
        </translation>
      </itext>
      <bind nodeset="/data/*qcode1*" type="string"/>
      <bind nodeset="/data/*qcode2*" type="string"/>
      <bind nodeset="/data/*qcode3*" type="int"/>
      <bind nodeset="/data/*qcode4*" type="decimal"/>
      <bind nodeset="/data/*qcode5*" type="geopoint"/>
      <bind nodeset="/data/*qcode6*" type="select1"/>
      <bind nodeset="/data/*qcode7*_1" type="select1"/>
      <bind nodeset="/data/*qcode7*_2" type="string"/>
      <bind nodeset="/data/*qcode8*" type="select"/>
      <bind nodeset="/data/*qcode9*" type="dateTime"/>
      <bind nodeset="/data/*qcode10*" type="date"/>
      <bind nodeset="/data/*qcode11*" type="time"/>
    </model>
  </h:head>
  <h:body>
    <input ref="/data/*qcode1*">
      <label ref="jr:itext('*qcode1*:label')"/>
      <hint ref="jr:itext('*qcode1*:hint')"/>
    </input>
    <input ref="/data/*qcode2*" rows="5">
      <label ref="jr:itext('*qcode2*:label')"/>
      <hint ref="jr:itext('*qcode2*:hint')"/>
    </input>
    <input ref="/data/*qcode3*">
      <label ref="jr:itext('*qcode3*:label')"/>
      <hint ref="jr:itext('*qcode3*:hint')"/>
    </input>
    <input ref="/data/*qcode4*">
      <label ref="jr:itext('*qcode4*:label')"/>
      <hint ref="jr:itext('*qcode4*:hint')"/>
    </input>
    <input ref="/data/*qcode5*">
      <label ref="jr:itext('*qcode5*:label')"/>
      <hint ref="jr:itext('*qcode5*:hint')"/>
    </input>
    <select1 ref="/data/*qcode6*">
      <label ref="jr:itext('*qcode6*:label')"/>
      <hint ref="jr:itext('*qcode6*:hint')"/>
      <item>
        <label ref="jr:itext('*optcode1*')"/>
        <value>*optcode1*</value>
      </item>
      <item>
        <label ref="jr:itext('*optcode2*')"/>
        <value>*optcode2*</value>
      </item>
    </select1>
    <select1 ref="/data/*qcode7*_1">
      <label ref="jr:itext('*qcode7*_1:label')"/>
      <hint ref="jr:itext('*qcode7*_1:hint')"/>
      <item>
        <label ref="jr:itext('*optcode6*')"/>
        <value>*optcode6*</value>
      </item>
      <item>
        <label ref="jr:itext('*optcode3*')"/>
        <value>*optcode3*</value>
      </item>
    </select1>
    <input ref="/data/*qcode7*_2" query="instance('os2')/root/item[parent_id=/data/*qcode7*_1]">
      <label ref="jr:itext('*qcode7*_2:label')"/>
      <hint ref="jr:itext('*qcode7*_2:hint')"/>
    </input>
    <select ref="/data/*qcode8*">
      <label ref="jr:itext('*qcode8*:label')"/>
      <hint ref="jr:itext('*qcode8*:hint')"/>
      <item>
        <label ref="jr:itext('*optcode9*')"/>
        <value>*optcode9*</value>
      </item>
      <item>
        <label ref="jr:itext('*optcode10*')"/>
        <value>*optcode10*</value>
      </item>
    </select>
    <input ref="/data/*qcode9*">
      <label ref="jr:itext('*qcode9*:label')"/>
      <hint ref="jr:itext('*qcode9*:hint')"/>
    </input>
    <input ref="/data/*qcode10*">
      <label ref="jr:itext('*qcode10*:label')"/>
      <hint ref="jr:itext('*qcode10*:hint')"/>
    </input>
    <input ref="/data/*qcode11*">
      <label ref="jr:itext('*qcode11*:label')"/>
      <hint ref="jr:itext('*qcode11*:hint')"/>
    </input>
  </h:body>
</h:html>
