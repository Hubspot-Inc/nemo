{
  "env": {
    "production": {
      "presets": [
        [
          "env",
          {
            "modules": false,
            "targets": {
              "browsers": "> 1%",
              "uglify": true
            },
            "useBuiltIns": true
          }
        ],
        "react"
      ],
      "plugins": [
        "syntax-dynamic-import",
        "transform-runtime",
        "transform-decorators-legacy",
        "transform-object-rest-spread",
        [
          "transform-class-properties",
          {
            "loose": true
          }
        ]
      ]
    },
    "development": {
      "presets": [
        [
          "env",
          {
            "modules": false,
            "targets": {
              "browsers": "> 1%",
              "uglify": true
            },
            "useBuiltIns": true
          }
        ],
        "react"
      ],
      "plugins": [
        "syntax-dynamic-import",
        "transform-runtime",
        "transform-decorators-legacy",
        "transform-object-rest-spread",
        [
          "transform-class-properties",
          {
            "loose": true
          }
        ]
      ]
    },
    "test": {
      "presets": [
        [
          "env",
          {
            "targets": {
              "browsers": "> 1%",
              "uglify": true
            },
            "useBuiltIns": true
          }
        ],
        "react"
      ],
      "plugins": [
        "syntax-dynamic-import",
        "transform-runtime",
        "transform-decorators-legacy",
        "transform-object-rest-spread",
        [
          "transform-class-properties",
          {
            "loose": true
          }
        ]
      ]
    }
  }
}
