<% items.each do |item| %>
  <% if item.is_a?(Questioning) %>
    <%= render "questionings/printable", qing: item.decorate %>
  <% elsif item.is_a?(QingGroup) %>
    <%= render "qing_groups/printable", qing_group: item %>
    <% if item.children? %>
      <%= render "form_items/printable", items: item.visible_children %>
    <% end %>
  <% end %>
<% end %>
