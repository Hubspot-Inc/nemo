<% items.each do |item, subitems| %>
  <% if item.is_a?(QingGroup) %>
    <% if !subitems.empty? %>
      <%= render 'qing_groups/printable', qing_group: item %>
      <%= render 'form_items/printable', items: subitems %>
    <% end %>
  <% elsif item.printable? %>
    <%= render 'questionings/printable', qing: Printable::QingDecorator.new(item) %>
  <% end %>
<% end %>
